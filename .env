# Create this file and add your credentials
GRPC_URL=your_grpc_url_here
X_TOKEN=your_access_token_here
# RPC URL for loading address lookup tables (e.g., https://api.mainnet-beta.solana.com or your QuickNode URL)
RPC_URL=https://api.mainnet-beta.solana.com
# Set to false to disable ALT resolution even if RPC_URL is provided
ALT_RESOLUTION=true
# RPC rate limit (calls per second) - default is 10
RPC_RATE_LIMIT=10
# Set to true to enable debug logging for ALT resolution
DEBUG=false